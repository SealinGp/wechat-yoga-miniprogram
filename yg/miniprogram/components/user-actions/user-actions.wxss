.action {
    height: 64rpx;
    min-width: 64rpx;
    padding: 68rpx 0 0;
    font-size: 24rpx;
    line-height: 32rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    background-position: center 8rpx;
    background-size: 48rpx 48rpx;
    background-repeat: no-repeat;
    text-align: center;
    color: #333;
    background-image: url(data:image/svg+xml;base64,);
  }
  
  .action-feedback {
    background-image: url(data:image/svg+xml;base64,);
  }
  .action-check {
    background-image: url(data:image/svg+xml;base64,);
  }
  .action-history {
    background-image: url(data:image/svg+xml;base64,);
  }
  .action-fav {
    background-image: url(data:image/svg+xml;base64,);
  }