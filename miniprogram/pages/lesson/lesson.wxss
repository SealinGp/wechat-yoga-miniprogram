page {
  background: #fff;
}
.cover {
  height: 336rpx;
}
.top-header {
  margin: 40rpx 48rpx 0;
}
.top-title {
  font: 400 28rpx/40rpx Roboto, Arial, sans-serif;
  margin: 0;
  padding: 0;
  color: #202124;
  font-size: 40rpx;
  font-weight: 400;
  line-height: 56rpx;
  margin-bottom: 8rpx;
}
.top-subtitle {
  font: 400 28rpx/40rpx Roboto, Arial, sans-serif;
  color: #70757a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
  margin: -28rpx 0;
}
.top-subtitle-container {
  display: flex;
  align-items: center;
  padding: 28rpx 0;
  font-size: 28rpx;
  line-height: 40rpx;
}
.ajphuy {
  font: 400 28rpx/40rpx Roboto, Arial, sans-serif;
  color: #70757a;
  display: inline-block;
  font-size: 30rpx;
  font-weight: 500;
  overflow: hidden;
  position: relative;
  line-height: 40rpx;
  font-family: "Google Sans", Roboto, Arial, sans-serif;
  border: 1px solid #dadce0;
  border-radius: 200rpx;
  border-color: #749b15;
  margin: 32rpx 48rpx 0;
  text-align: center;
}
.fkaebx {
  margin: 18rpx 40rpx;
  font: 400 24rpx/32rpx Roboto, Arial, sans-serif;
  color: #749b15;
  padding: 0 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
}
.header {
  font: 500 32rpx/40rpx "Google Sans", Roboto, Arial, sans-serif;
  color: #202124;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 34rpx 30rpx 30rpx 30rpx;
}
.comments {
  font: 400 28rpx/40rpx Roboto, Arial, sans-serif;
  color: #70757a;
  padding: 0 30rpx 30rpx 30rpx;
}
.comment-item {
  border-bottom: solid 2rpx #dadce0;
  padding: 20rpx 0;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #3c4043;
}
.comment-item:first-child {
  border-top: solid 2rpx #dadce0;
}
.comment-wrapper {
  display: flex;
}
.comment-left {
  width: 80rpx;
  height: 80rpx;
  border: none;
  background-color: red;
  border-radius: 50%;
  flex-shrink: 0;
  overflow: hidden;
}
.comment-right {
  font-size: 26rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #3c4043;
  margin-left: 20rpx;
  flex-grow: 1;
}
.comment-add-wrapper {
  color: #749b15;
  font-size: 28rpx;
  font-weight: 500;
  padding: 0 32rpx 0 24rpx;
  display: flex;
  align-items: center;
}
.photos {
  font: 400 28rpx/40rpx Roboto, Arial, sans-serif;
  color: #70757a;
  display: flex;
  overflow-x: auto;
  overflow-y: hidden;
  padding-bottom: 34rpx;
  white-space: nowrap;
}
.photos::-webkit-scrollbar {
  display: none;
}
.photo {
  border-radius: 16rpx;
  box-shadow: 0 2rpx 12rpx 0 rgba(0, 0, 0, 0.16);
  margin-right: 16rpx;
  display: inline-block;
  height: 304rpx;
  overflow: hidden;
  position: relative;
  width: 304rpx;
  flex-shrink: 0;
}

.photo:first-child {
  margin-left: 32rpx;
}
.divider {
  height: 32rpx;
  border-bottom: 2rpx solid #dadce0;
}
.comments-subtitle {
  color: #757575;
}
.wztfuh {
  color: #70757a;
  text-align: center;
  height: 20vh;
  display: flex;
  align-items: center;
  justify-content: center;

  border-top: solid 2rpx #dadce0;
}
.cover-image {
  width: 100%;
  height: 100%;
}
