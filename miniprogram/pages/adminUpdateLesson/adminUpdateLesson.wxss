page {
  background: rgb(247, 248, 250)
}

.h2 {


  padding: 40rpx 32rpx 32rpx;
  color: rgb(150, 151, 153);
  font-size: 28rpx;
  line-height: 32rpx;

}

.cell {

  position: relative;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx 32rpx;
  overflow: hidden;
  color: #323233;
  font-size: 28rpx;
  line-height: 48rpx;
  background: #fff;
  cursor: pointer;

}

.cell__title {
  flex: 1;

}

.van-icon-arrow {

  position: relative;
  display: inline-block;
  height: 48rpx;
  width: 32rpx;
  margin-left: 8rpx;
  color: #969799;
  background-size: 100% 100%;
  background-image: url(data:image/svg+xml;base64,)
}
.btn-wrapper{

margin: 32rpx 32rpx 0rpx;

    }
.btn{
    -webkit-tap-highlight-color: transparent;
position: relative;
box-sizing: border-box;
height: 88rpx;
color: #fff;
background: #1989fa;
border: 2rpx solid #1989fa;
padding: 0 30rpx;
font-size: 28rpx;
display: flex;
width: 100%;
border-radius: 1998rpx;
align-items:center;
justify-content:center;
    }
