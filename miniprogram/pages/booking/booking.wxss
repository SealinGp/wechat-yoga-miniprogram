page {
  display: flex;
  flex-direction: column;
  height: 100%
}

.empty-container {
  flex-grow: 1;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.empty-container>view {

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.empty-picture {
  width: 128rpx;
  height: 128rpx;
  margin-bottom: 64rpx;
}


.empty-text {
  color: rgba(0, 0, 0, 0.35);
}