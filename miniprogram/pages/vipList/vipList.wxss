}

.secondary-container {

    padding-bottom: 12rpx;
}

.bottom-content {
 
    align-items: baseline;
    display: block;
    position: relative;
    z-index: 2;
    padding: 16rpx 16rpx 0 0;
    padding-top: 16rpx;
    border-top: 2rpx solid #e8eaed;
}
