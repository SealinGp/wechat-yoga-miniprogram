
.item {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
    max-height: 1999998rpx;
    -webkit-tap-highlight-color: rgba(0, 0, 0, .00);
    border: 2rpx solid #dadce0;
    border-radius: 16rpx;
    margin-bottom: 16rpx;
    border-bottom: 2rpx solid #ebebeb;
    color: #3c4043;
    font-size: 28rpx;
    line-height: 40rpx;
    list-style: none;
    position: relative;
    word-wrap: break-word;
    width: 100%;
    background-color: #fff;
    overflow: hidden;
    z-index: 1;
}

.link {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
    font-size: 28rpx;
    line-height: 40rpx;
    list-style: none;
    word-wrap: break-word;
    max-height: 1999998rpx;
    color: #1558d6;
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, .10);
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1;
}

.table {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
    -webkit-tap-highlight-color: rgba(0, 0, 0, .00);
    color: #3c4043;
    font-size: 28rpx;
    line-height: 40rpx;
    list-style: none;
    word-wrap: break-word;
    max-height: 1999998rpx;
    display: table;
    background-color: #fff;
}

.left {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
    -webkit-tap-highlight-color: rgba(0, 0, 0, .00);
    color: #3c4043;
    font-size: 28rpx;
    line-height: 40rpx;
    list-style: none;
    word-wrap: break-word;
    max-height: 1999998rpx;
    position: relative;
    display: table-cell;
    width: 232rpx;
    vertical-align: middle;
    padding-right: 24rpx;
}

.right {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
    -webkit-tap-highlight-color: rgba(0, 0, 0, .00);
    color: #3c4043;
    line-height: 40rpx;
    list-style: none;
    word-wrap: break-word;
    max-height: 1999998rpx;
    font-size: 26rpx;
    margin-bottom: 0;
    display: table-cell;
    vertical-align: top;
    max-width: calc(100vw - 340rpx);
    width: 100%;
    padding-right: 16rpx;
    padding-top: 16rpx;
}

.thumbnail {
    font-family: Roboto, Helvetica Neue, Arial, sans-serif;
