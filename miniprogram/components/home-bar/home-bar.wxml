<view class="wrapper" style="height:{{height}};padding-top:{{top}}">
    <view class="item-wrapper">
        <view>{{app.globalData.title}}</view>
        <swiper class="swiper" vertical="true" circular="true" duration="300" autoplay="true" interval="3000">
            <swiper-item>{{weather}}</swiper-item>
            <swiper-item>{{date}}</swiper-item>
            <swiper-item>{{bj}}</swiper-item>
        </swiper>
    </view>
</view>